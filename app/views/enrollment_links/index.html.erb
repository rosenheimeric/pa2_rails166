<p id="notice"><%= notice %></p>

<h1>Enrollment Links</h1>

<table>
  <thead>
    <tr>
      <th>Course list</th>
      <th></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @enrollment_links.each do |enrollment_link| %>
      <tr>
        <td><%= enrollment_link.course_list %></td>
        <td><%= enrollment_link. %></td>
        <td><%= link_to 'Show', enrollment_link %></td>
        <td><%= link_to 'Edit', edit_enrollment_link_path(enrollment_link) %></td>
        <td><%= link_to 'Destroy', enrollment_link, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Enrollment Link', new_enrollment_link_path %>
